<h3> Little Esty Shop</h3>
<h3> Name: <%= @merchant.name %></h3>
<%= link_to "Merchant Items", "/merchants/#{@merchant.id}/items" %>
<%= link_to "Merchant Invoices", "/merchants/#{@merchant.id}/invoices" %>

<h3>Top 5 Customers</h3>
<% top = @merchant.top_five_merchant_customers_by_transaction(@merchant.id) %>
<% top.each do |customer| %>
  <%= "#{customer.first_name} #{customer.last_name} - #{customer.transaction_count} successful transactions" %>
  <br>
<% end %>
